{
  "name": "scripts-collection",
  "version": "1.0.0",
  "description": "A curated collection of excellent JavaScript scripts",
  "keywords": [
    "javascript",
    "scripts",
    "code-examples"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run gf && npm run gd && vitepress build docs && npm run gr",
    "dev": "vitepress dev docs",
    "generate": "node scripts/generate-docs.js",
    "preview": "vitepress preview docs",
    "ship": "wrangler pages deploy",
    "test": "node --test test/**/*.test.js",
    "test:docs": "node --test test/docs.test.js",
    "test:features": "node --test test/update-features.test.js",
    "gd": "node scripts/generate-docs.js",
    "gf": "node scripts/generate-features.js",
    "gr": "node scripts/generate-robots.mjs"
  },
  "devDependencies": {
    "terser": "^5.44.1",
    "vitepress": "^1.6.4"
  }
}
